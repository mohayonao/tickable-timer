{
  "name": "tickable-timer",
  "description": "Manual ticking timer API",
  "version": "0.2.0",
  "author": "mohayonao <mohayonao@gmail.com>",
  "bugs": {
    "url": "https://github.com/mohayonao/tickable-timer/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "babel": "^5.1.13",
    "browserify": "^9.0.8",
    "espower-babel": "^2.1.0",
    "mocha": "^2.2.4",
    "power-assert": "^0.11.0",
    "uglify-js": "^2.4.20"
  },
  "files": [
    "package.json",
    "README.md",
    "lib"
  ],
  "homepage": "https://github.com/mohayonao/tickable-timer/",
  "keywords": [
    "timer",
    "setInterval",
    "setTimeout",
    "clearInterval",
    "clearTimeout",
    "test"
  ],
  "license": "MIT",
  "main": "lib/",
  "repository": {
    "type": "git",
    "url": "https://github.com/mohayonao/tickable-timer.git"
  },
  "scripts": {
    "build": "npm run build-to5 && npm run build-browser && npm run build-minify",
    "build-browser": "browserify lib --standalone TickableTimer -o build/tickable-timer.js",
    "build-minify": "uglifyjs build/tickable-timer.js -o build/tickable-timer.min.js",
    "build-to5": "babel src --out-dir lib --source-maps-inline",
    "test": "mocha --require espower-babel/guess",
    "travis": "npm run test"
  }
}
